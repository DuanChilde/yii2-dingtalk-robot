{
    "name": "ipaya/yii2-dingtalk-robot",
    "description": "钉钉自定义机器人",
    "keywords": [
        "yii2",
        "framework",
        "iPaya",
        "钉钉",
        "机器人"
    ],
    "type": "yii2-extension",
    "minimum-stability": "stable",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.0",
        "yiisoft/yii2": "~2.0.11",
        "yiisoft/yii2-httpclient": "*"
    },
    "require-dev": {
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-debug": "*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "iPaya\\DingTalk\\Robot\\": "src"
        }
    }
}
